{
  "name": "@xsky/ai-agent-monorepo",
  "version": "0.2.0",
  "description": "A production-ready JavaScript AI Agent framework for creating reliable agents across browser and Node.js environments.",
  "workspaces": [
    "packages/ai-agent-core",
    "packages/ai-agent-electron",
    "packages/ai-agent-extension",
    "packages/ai-agent-nodejs",
    "packages/ai-agent-web"
  ],
  "scripts": {
    "build": "pnpm -r --sequential build",
    "build-core": "pnpm --filter @xsky/ai-agent-core build",
    "test": "pnpm -r --sequential test",
    "bench": "npx tsx benchmarks/core.bench.ts",
    "clean": "rm -rf node_modules && pnpm -r exec -- rm -rf node_modules dist"
  },
  "author": "XSky",
  "license": "MIT",
  "keywords": [
    "xsky",
    "ai-agent",
    "automation",
    "browser-automation",
    "workflow",
    "llm",
    "agent-framework"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "tinybench": "^6.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.18.2+sha512.9fb969fa749b3ade6035e0f109f0b8a60b5d08a1a87fdf72e337da90dcc93336e2280ca4e44f2358a649b83c17959e9993e777c2080879f3801e6f0d999ad3dd",
  "pnpm": {
    "overrides": {
      "postcss": "^8.4.31",
      "nth-check": "^2.0.1",
      "webpack-dev-server": "^5.2.1"
    }
  }
}
